    "## Horner's Rule"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3ef9d0b4",
   "metadata": {},
   "outputs": [],
   "source": [
    "def HornerRule(coeff, x):\n",
    "  result = 0\n",
    "  for i in range(len(coeff)-1, -1, -1):\n",
    "    result = result * x + coeff[i]\n",
    "  return result"
   ]
  }
